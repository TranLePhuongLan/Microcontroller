    "import pyfirmata\n",
    "import time\n",
    "\n",
    "board = pyfirmata.Arduino('COM8')\n",
    "\n",
    "while True:\n",
    "    board.digital[13].write(1) # digital pin 13 = built-in LED\n",
    "    time.sleep(1)\n",
    "    board.digital[13].write(0)\n",
    "    time.sleep(1)"
